                var result = "";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#inputCarBenefitsSummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#numberCarBenefits").change(function (e) {

                var q = $("#numberCarBenefits").val();
