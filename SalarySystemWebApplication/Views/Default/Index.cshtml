                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#inputCarBenefitsSummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#numberDailyAllowance,#inputDailyAllowance").keyup(function (e) {

                var q = $("#numberDailyAllowance").val();
                var p = $("#inputDailyAllowance").val();
                var result = "0";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#inputDailyAllowanceSummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#numberDailyAllowance").change(function (e) {

                var q = $("#numberDailyAllowance").val();
                var p = $("#inputDailyAllowance").val();
                var result = "0";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#inputDailyAllowanceSummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#numberFixedSalary,#employeeSettings_fixedSalary").keyup(function (e) {

                var q = $("#Launalidur_heiti").val();
                var p = $("#numberFixedSalary").val();
                var t = $("#employeeSettings_fixedSalary").val();
                var result = "0";

                if (q !== "" && p !== "" && t !== "" && $.isNumeric(q) && $.isNumeric(p) && $.isNumeric(t)) {
                    result = parseFloat(p) * parseFloat(q) * parseFloat(t) * parseFloat(0.01);
                }
                $("#inputFixedSalarySummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#numberFixedSalary").change(function (e) {

                var q = $("#Launalidur_heiti").val();
                var p = $("#numberFixedSalary").val();
                var t = $("#employeeSettings_fixedSalary").val();
                var result = "0";

                if (q !== "" && p !== "" && t !== "" && $.isNumeric(q) && $.isNumeric(p) && $.isNumeric(t)) {
                    result = parseFloat(p) * parseFloat(q) * parseFloat(t) * parseFloat(0.01);
                }
                $("#inputFixedSalarySummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#Launalidur_heiti").change(function (e) {

                var q = $("#Launalidur_heiti").val();
                var p = $("#numberFixedSalary").val();
                var t = $("#employeeSettings_fixedSalary").val();
                var result = "0";

                if (q !== "" && p !== "" && t !== "" && $.isNumeric(q) && $.isNumeric(p) && $.isNumeric(t)) {
                    result = parseFloat(p) * parseFloat(q) * parseFloat(t) * parseFloat(0.01);
                }
                $("#inputFixedSalarySummary").val(result);

                calculate_summary();
                calculate_pension();

            });
            $("#otherPayments1").change(function (e) {

                var p = $("#otherPayments1").val();

                result = parseFloat(p);
                var result = "0";

                $("#inputOtherPaymentsList1").val(p);
                $("#inputOtherPaymentsList1Summary").val(p);

                calculate_summary();

            });
            $("#otherPayments2").change(function (e) {

                var p = $("#otherPayments2").val();

                result = parseFloat(p);
                var result = "0";

                $("#inputOtherPaymentsList2").val(p);
                $("#inputOtherPaymentsList2Summary").val(p);

                calculate_summary();

            });
            $("#inputOtherPaymentsList1").keyup(function (e) {

                var q = $("#inputOtherPaymentsList1").val();
