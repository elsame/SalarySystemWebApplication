    <script>
        $(function () {
            $("#overtime,#overtimetaxti").keyup(function (e) {

                var q = $("#overtime").val();
                var p = $("#overtimetaxti").val();
                var result = "0";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#overtimePay").val(result);

                calculate_summary();

            });
            $("#overtime").change(function (e) {

                var q = $("#overtime").val();
                var p = $("#overtimetaxti").val();
                var result = "0";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#overtimePay").val(result);

                calculate_summary();

            });
            function calculate_summary() {
                var summary = "0";
                var overtimepaySummary = $("#overtimePay").val();
                var fixedSalarySummary = $("#inputFixedSalarySummary").val();
                var ShiftWorkSummary = $("#inputShiftWorkSummary").val();
                var CarBenefitsSummary = $("#inputCarBenefitsSummary").val();
                var DailyAllowanceSummary = $("#inputDailyAllowanceSummary").val();
                var OtherPaymentsList1Summary = $("#inputOtherPaymentsList1Summary").val();
                var OtherPaymentsList2Summary = $("#inputOtherPaymentsList2Summary").val();

                summary = parseFloat(overtimepaySummary) + parseFloat(fixedSalarySummary) + parseFloat(ShiftWorkSummary) + parseFloat(CarBenefitsSummary)
                    + parseFloat(DailyAllowanceSummary) + parseFloat(OtherPaymentsList1Summary) + parseFloat(OtherPaymentsList2Summary);
                $("#inputSalaryAll").val(summary);
                $("#inputSalarySummaryMonth").val(summary);

                calculate_pension();
            }
            function calculate_pension() {
                var Premium = $("#inputPremium").val();
                var fixedSalary = $("#inputFixedSalarySummary").val();
                var carBenefits = $("#inputCarBenefitsSummary").val();
                var DailyAllowance = $("#inputDailyAllowanceSummary").val();

                var result = "0";
                var payNotRSK = parseFloat(fixedSalary);

                if (carBenefits !== "" && $.isNumeric(carBenefits)) {
                    payNotRSK = parseFloat(payNotRSK) - parseFloat(carBenefits);
                }

                if (DailyAllowance !== "" && $.isNumeric(DailyAllowance)) {
                    payNotRSK = parseFloat(payNotRSK) - parseFloat(DailyAllowance);
                }

                result = parseFloat(Premium) * parseFloat(payNotRSK) * parseFloat(0.01);

                $("#inputPensionPayment").val(result);
                $("#inputPensionPaymentMonth").val(result);

                calculate_privatepension();
            }
            function calculate_privatepension() {
                var Premium = $("#inputprivateProperty").val();
                var fixedSalary = $("#inputFixedSalarySummary").val();
                var carBenefits = $("#inputCarBenefitsSummary").val();
                var DailyAllowance = $("#inputDailyAllowanceSummary").val();

                var result = "0";
                var payNotRSK = parseFloat(fixedSalary);

                if (carBenefits !== "" && $.isNumeric(carBenefits)) {
                    payNotRSK = parseFloat(payNotRSK) - parseFloat(carBenefits);
                }

                if (DailyAllowance !== "" && $.isNumeric(DailyAllowance)) {
                    payNotRSK = parseFloat(payNotRSK) - parseFloat(DailyAllowance);
                }

                result = parseFloat(Premium) * parseFloat(payNotRSK) * parseFloat(0.01);

                $("#inputPensionPaymentPrivate").val(result);
                $("#inputPensionPaymentPrivateMonth").val(result);

                calculate_notRSK();
            }
            function calculate_notRSK() {
                var Premium = $("#inputPensionPayment").val();
                var privateFunds = $("#inputPensionPaymentPrivate").val();
                var carBenefits = $("#inputCarBenefitsSummary").val();
                var DailyAllowance = $("#inputDailyAllowanceSummary").val();

                var result = "0";

                result = parseFloat(carBenefits) + parseFloat(DailyAllowance) + parseFloat(Premium) + parseFloat(privateFunds);

                $("#inputPaymentNotRSK").val(result);
                $("#inputPaymentNotRSKMonth").val(result);

                calculate_RSK();
            }
            function calculate_RSK() {
                var PaymentNotRSK = $("#inputPaymentNotRSK").val();
                var Payment = $("#inputFixedSalarySummary").val();

                var result = "0";

                result = parseFloat(Payment) - parseFloat(PaymentNotRSK);

                $("#inputPaymentRSK").val(result);
                $("#inputPaymentRSKMonth").val(result);

                calculate_taxrate();
            }
            function calculate_taxrate() {
                var Payment = $("#inputPaymentRSK").val();

                var taxStep = "927087";
                var taxRate = "0.3694";
                var taxRate2 = "0.4624";

                var result = "0";

                if (parseFloat(Payment) <= parseFloat(taxStep))
                    result = parseFloat(Payment) * parseFloat(taxRate);
                else
                    result = ((parseFloat(Payment) - parseFloat(taxStep)) * parseFloat(taxRate2)) + (parseFloat(taxStep) * parseFloat(taxRate));

                $("#inputPaymentTaxRates").val(result);
                $("#inputPaymentTaxRatesMonth").val(result);

                calculate_TaxRatesC();
            }
            function calculate_TaxRatesC() {
                var PaymentTaxRates = $("#inputPaymentTaxRates").val();
                var personalDiscount = $("#inputPersonDiscount").val();

                result = parseFloat(PaymentTaxRates) - parseFloat(personalDiscount);

                $("#inputPaymentTaxRatesCMonth").val(result);

                calculate_MembershipFee();

            }
            function calculate_MembershipFee() {
                var FixedSalarySummary = $("#inputFixedSalarySummary").val();
                var MembershipFee = $("#MembershipFee").val();

                result = parseFloat(FixedSalarySummary) * parseFloat(MembershipFee) * parseFloat("0.01");

                $("#inputMembershipFee").val(result);
                $("#inputMembershipFeeMonth").val(result);

            }
            $("#numberShiftWork,#inputShiftWork").keyup(function (e) {

                var q = $("#numberShiftWork").val();
                var p = $("#inputShiftWork").val();
                var result = "0";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#inputShiftWorkSummary").val(result);

                calculate_summary();

            });
            $("#numberShiftWork").change(function (e) {

                var q = $("#numberShiftWork").val();
                var p = $("#inputShiftWork").val();
                var result = "0";

                if (q !== "" && p !== "" && $.isNumeric(q) && $.isNumeric(p)) {
                    result = parseFloat(q) * parseFloat(p);
                }
                $("#inputShiftWorkSummary").val(result);

